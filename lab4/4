from datetime import datetime

date1 = datetime(2025, 2, 18, 12, 0, 0)  # February 18, 2025 at 12:00 PM
date2 = datetime(2025, 2, 19, 14, 30, 0)  # February 19, 2025 at 2:30 PM

difference = date2 - date1

seconds_difference = difference.total_seconds()

print("Difference in seconds:", seconds_difference)
